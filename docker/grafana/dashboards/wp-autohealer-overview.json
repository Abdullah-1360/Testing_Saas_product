{
  "dashboard": {
    "id": null,
    "title": "WP-AutoHealer System Overview",
    "tags": ["wp-autohealer", "monitoring", "overview"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "System Status",
        "type": "stat",
        "targets": [
          {
            "expr": "wp_autohealer_system_health_score",
            "legendFormat": "Health Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.9}
              ]
            },
            "unit": "percentunit",
            "min": 0,
            "max": 1
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Active Incidents",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(wp_autohealer_incidents{state!=\"FIXED\",state!=\"ESCALATED\"})",
            "legendFormat": "Active"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(wp_autohealer_errors_total[5m]) * 60",
            "legendFormat": "Errors/min"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 20}
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Response Time (95th percentile)",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(wp_autohealer_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 3}
              ]
            },
            "unit": "s"
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 5,
        "title": "HTTP Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(wp_autohealer_http_requests_total[5m])",
            "legendFormat": "{{method}} {{route}}"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 6,
        "title": "Incident Processing Time",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(wp_autohealer_incident_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(wp_autohealer_incident_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "P95"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 7,
        "title": "Database Health",
        "type": "graph",
        "targets": [
          {
            "expr": "wp_autohealer_database_health_status",
            "legendFormat": "DB Health"
          },
          {
            "expr": "wp_autohealer_database_response_time_ms",
            "legendFormat": "Response Time (ms)"
          }
        ],
        "yAxes": [
          {
            "label": "Status / Time",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 16}
      },
      {
        "id": 8,
        "title": "Redis Health",
        "type": "graph",
        "targets": [
          {
            "expr": "wp_autohealer_redis_health_status",
            "legendFormat": "Redis Health"
          },
          {
            "expr": "wp_autohealer_redis_response_time_ms",
            "legendFormat": "Response Time (ms)"
          }
        ],
        "yAxes": [
          {
            "label": "Status / Time",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 16}
      },
      {
        "id": 9,
        "title": "Queue Status",
        "type": "graph",
        "targets": [
          {
            "expr": "wp_autohealer_queue_jobs_active",
            "legendFormat": "Active Jobs"
          },
          {
            "expr": "wp_autohealer_queue_jobs_waiting",
            "legendFormat": "Waiting Jobs"
          }
        ],
        "yAxes": [
          {
            "label": "Jobs",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 16}
      },
      {
        "id": 10,
        "title": "Security Events",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(wp_autohealer_security_events_total[5m]) * 60",
            "legendFormat": "{{type}} - {{severity}}"
          }
        ],
        "yAxes": [
          {
            "label": "Events/min",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 11,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "wp_autohealer_process_memory_usage_bytes{type=\"heapUsed\"}",
            "legendFormat": "Heap Used"
          },
          {
            "expr": "wp_autohealer_process_memory_usage_bytes{type=\"heapTotal\"}",
            "legendFormat": "Heap Total"
          },
          {
            "expr": "wp_autohealer_process_memory_usage_bytes{type=\"rss\"}",
            "legendFormat": "RSS"
          }
        ],
        "yAxes": [
          {
            "label": "Bytes",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      }
    ],
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "schemaVersion": 27,
    "version": 1,
    "links": []
  }
}