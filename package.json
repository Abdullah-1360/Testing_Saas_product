{
  "name": "wp-autohealer",
  "version": "1.0.0",
  "description": "Production-grade WordPress self-healing system that functions as an autonomous Level-1.5 SRE/Support Engineer",
  "main": "dist/main.js",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:pbt": "jest --testNamePattern=\"Property\" --verbose",
    "test:integration": "./scripts/run-integration-tests.sh",
    "test:integration:workflow": "jest --config ./test/integration/jest-integration.json test/integration/incident-workflow.e2e-spec.ts",
    "test:integration:migration": "jest --config ./test/integration/jest-integration.json test/integration/database-migration.e2e-spec.ts",
    "test:integration:realtime": "jest --config ./test/integration/jest-integration.json test/integration/realtime-updates.e2e-spec.ts",
    "test:integration:auth": "jest --config ./test/integration/jest-integration.json test/integration/api-authorization.e2e-spec.ts",
    "test:integration:coverage": "jest --config ./test/integration/jest-integration.json --coverage test/integration/",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "typecheck": "tsc --noEmit"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@bull-board/api": "^5.10.2",
    "@bull-board/express": "^5.10.2",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/throttler": "^5.1.1",
    "@prisma/client": "^5.7.1",
    "@types/qrcode": "^1.5.6",
    "bcrypt": "^5.1.1",
    "bullmq": "^4.15.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "libsodium-wrappers": "^0.7.11",
    "nodemailer": "^7.0.13",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.2",
    "speakeasy": "^2.0.0",
    "ssh2": "^1.15.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/eventsource": "^1.1.15",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/joi": "^17.2.3",
    "@types/libsodium-wrappers": "^0.7.13",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^7.0.9",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-local": "^1.0.38",
    "@types/speakeasy": "^2.0.10",
    "@types/ssh2": "^1.11.19",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eventsource": "^4.1.0",
    "fast-check": "^3.15.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "prisma": "^5.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "wordpress",
    "self-healing",
    "sre",
    "automation",
    "nestjs",
    "typescript",
    "devops"
  ],
  "author": "WP-AutoHealer Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wp-autohealer/wp-autohealer.git"
  }
}
